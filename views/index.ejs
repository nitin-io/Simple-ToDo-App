<%- include('header.ejs') -%>
        <div class="box">
                <h1 id='heading'><%= listTitle %></h1>
        </div>
        <div class="box">
                <% for(let i = 0; i < newTodoItems.length; i++){ %>
                        <div class="item">
                                <input type="checkbox"><p> <%= newTodoItems[i] %> </p>
                        </div>
               <% } %>
        
        <form class="item" action="/" method="post">
                <input type="text" name="todo" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>

        </div>
<%- include('footer.ejs') -%>